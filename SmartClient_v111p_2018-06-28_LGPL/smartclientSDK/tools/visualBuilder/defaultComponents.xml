		        </PaletteNode>
		        <PaletteNode>
		            <title>Label</title>
		            <type>WindowHeaderLabel</type>
		            <icon>ui-label.png</icon>
		            <showDropIcon>true</showDropIcon>
		        </PaletteNode>
		        <PaletteNode>
		            <title>Minimize Button</title>
		            <type>WindowMinimizeButton</type>
		            <icon>[SKINIMG]Window/minimize.png</icon>
		            <showDropIcon>true</showDropIcon>
		        </PaletteNode>
