		            <title>ToolStripButton</title>
		            <type>ToolStripButton</type>
		            <icon>button.gif</icon>
		        </PaletteNode>
		        <PaletteNode>
		            <title>ToolStripMenuButton</title>
                    <children>
                        <PaletteNode>
                            <parentName>0</parentName>
