<PaletteNode>
    <title>&lt;b&gt;Absolute Form&lt;/b&gt;</title>
    <idPrefix>AbsoluteForm</idPrefix>
    <type>DynamicForm</type>
    <icon>DynamicForm.png</icon>
    <showDropIcon>true</showDropIcon>
    <defaults>
        <itemLayout>absolute</itemLayout>
        <height>100</height>
        <snapVGap>8</snapVGap>
        <snapHGap>8</snapHGap>
        <snapHDirection>nearest</snapHDirection>
        <snapVDirection>nearest</snapVDirection>
    </defaults>
</PaletteNode>

