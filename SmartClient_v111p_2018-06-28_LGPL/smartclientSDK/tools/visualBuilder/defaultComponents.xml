<!-- list of default components for the visual builder component library -->
<PaletteNodes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<PaletteNode>
    <title>&lt;b&gt;ListGrid&lt;/b&gt;</title>
    <type>ListGrid</type>
    <icon>ListGrid.png</icon>
    <showDropIcon>true</showDropIcon>
    <children>
        <PaletteNode>
            <title>ListGridField</title>
            <type>ListGridField</type>
            <icon>text.gif</icon>
            <iconSize>18</iconSize>
            <showDropIcon>true</showDropIcon>
        </PaletteNode>
    </children>
</PaletteNode>

<PaletteNode>
    <title>&lt;b&gt;Editable ListGrid&lt;/b&gt;</title>
    <type>ListGrid</type>
    <icon>ListGrid.png</icon>
    <showDropIcon>true</showDropIcon>
    <defaults xsi:type="ListGrid">
        <showFilterEditor>true</showFilterEditor>
        <canEdit>true</canEdit>
        <canRemoveRecords>true</canRemoveRecords>
        <listEndEditAction>next</listEndEditAction>
    </defaults>
</PaletteNode>

